<% provide(:title, 'Admin users') %>

<div  class="container">
<table class="table table-striped table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>名前</th>
      <th>メールアドレス</th>
    </tr>
  </thead>
      <% @users.each do |user| %>
  <tbody>
        <tr>
          <td><%= user.id %></td>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
        </tr>
      <% end %>
  </tbody>

</table>

</div>
