<% provide(:title, 'Admin stocks') %>

<div  class="container">
<table class="table table-striped table-bordered table-hover table-condensed">
  <thead>
    <tr>
      <th>ユーザ名</th>
      <th>商品名</th>
    </tr>
  </thead>
      <% @stocks.each do |stock| %>
  <tbody>
        <tr>
          <td><%= stock.user.name %></td>
          <td><%= stock.user_item.name %></td>
        </tr>
      <% end %>
  </tbody>

</table>

</div>
